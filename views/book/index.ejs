<h1>Search Book</h1>
<form action="/books" method="GET">
  <label for="">Title</label>
  <input type="text" name="title" value="<%= searchOptions.title %>" />
  <div>
    <label for="publishDate">Publish After</label>
    <input
      type="date"
      name="publishAfter"
      value="<%= searchOptions.publishAfter %>"
    />
  </div>
  
    <label for="publishDate">Publish Before</label>
    <input
      type="date"
      name="publishBefore"
      value="<%= searchOptions.publishBefore %>"
    />
  </div>
  <button type="submit">Search</button>
</form>
<br />
<div>

<% books.forEach((book) => { %>
<img width="100" height="150" src="<%= book.coverImagePath %>" alt="<% book.title %>">
<% }) %>
</div>